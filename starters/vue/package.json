{
  "name": "vue-ts-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite build && vite preview",
    "lint": "eslint . && stylelint --allow-empty-input \"**/*.{css,less,scss}\"",
    "lint:fix": "prettier  . && eslint --fix . && stylelint --allow-empty-input --fix \"**/*.{css,less,scss}\"",
    "ci:eslint": "eslint -f json src -o ./.ci/eslint.json",
    "prepare": "husky"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": "prettier-config-ali",
  "stylelint": {
    "extends": [
      "stylelint-config-ali",
      "stylelint-prettier/recommended"
    ]
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "axios": "^1.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.545.0",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "reka-ui": "^2.5.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "unplugin-vue-components": "^29.0.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.35.0",
    "eslint-config-ali": "^16.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.6.2",
    "prettier-config-ali": "^1.5.0",
    "sass-embedded": "^1.83.0",
    "stylelint": "^16.23.1",
    "stylelint-config-ali": "^2.3.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.6.3",
    "unplugin-auto-import": "^0.19.0",
    "vite": "^6.0.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.1.10"
  },
  "packageManager": "pnpm@10.24.0"
}
